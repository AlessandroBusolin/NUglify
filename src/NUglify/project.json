{
  "title": "NUglify",
  "version": "1.2.0",
  "authors": [ "Ron Logan", "Alexandre Mutel" ],
  "owners": [ "Alexandre Mutel" ],
  "licenseUrl": "https://github.com/xoofx/NUglify/blob/master/license.txt",
  "projectUrl": "https://github.com/xoofx/NUglify",
  "requireLicenseAcceptance": false,
  "description": "NUglify is a HTML, JavaScript and CSS minification Library for .NET (fork of AjaxMin + new features)",
  "copyright": "Alexandre Mutel",
  "tags": [ "CSS", "JavaScript", "minify", "crunch", "uglify", "HTML", "htmlminifier", "htmlcompressor" ],
  "language": "en-US",
  "repository": {
    "type": "git",
    "url": "git://github.com/xoofx/NUglify"
  },
  "configurations": {
    "Debug": {
      "compilationOptions": {
        "define": [ "DEBUG", "TRACE" ],
        "allowUnsafe": true
      }
    },
    "Release": {
      "compilationOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true,
        "allowUnsafe": true,
        "xmlDoc": true,
        "keyFile": "key.snk",
        "strongName": true
      }
    }
  },
  "runtimes": {
    "win": {}
  },
  "resource": [
    "*.resx",
    "Css/*.resx",
    "JavaScript/*.resx"
  ],
  "namedResource": {
    "NUglify.Css.CssStrings": "Css/CssStrings.resx",
    "NUglify.CommonStrings": "CommonStrings.resx",
    "NUglify.JavaScript.JScript": "JavaScript/JScript.resx"
  },
  "frameworks": {
    "net20": {
      "compilationOptions": {
        "define": [ "NET_20", "NETPRE45" ]
      },
      "frameworkAssemblies": {
        "mscorlib": "",
        "System": "",
        "System.Core": "",
        "System.Xml": ""
      }
    },
    "net35": {
      "compilationOptions": {
        "define": [ "NET_35", "NETPRE45" ]
      },
      "frameworkAssemblies": {
        "mscorlib": "",
        "System": "",
        "System.Core": "",
        "System.Xml": ""
      }
    },
    "net40": {
      "compilationOptions": {
        "define": [ "NETPRE45" ]
      },
      "frameworkAssemblies": {
        "mscorlib": "",
        "System": "",
        "System.Core": "",
        "System.Xml": ""
      }
    },
    "dotnet5.4": {
      "compilationOptions": {
        "define": [ "CORE" ]
      },
      "dependencies": {
        "Microsoft.NETCore": "5.0.1-beta-23516",
        "Microsoft.NETCore.Platforms": "1.0.1-beta-23516",
        "System.Runtime.Extensions": "4.0.11-beta-23516",
        "System.Reflection": "4.1.0-beta-23516",
        "System.Collections": "4.0.11-beta-23516",
        "System.Linq": "4.0.1-beta-23516",
        "System.Threading": "4.0.11-beta-23516",
        "System.Runtime": "4.0.21-beta-23516",
        "System.Xml.ReaderWriter": "4.0.11-beta-23516",
        "Microsoft.CSharp": "4.0.1-beta-23516",
        "System.Globalization.Extensions": "4.0.1-beta-23516",
        "System.Security.Cryptography.Encoding": "4.0.0-beta-23516",
        "System.Security.Cryptography.Algorithms": "4.0.0-beta-23516"
      }
    }
  }
}