{
  "title": "AjaxMin",
  "version": "1.0.1",
  "authors": [ "Alexandre Mutel" ],
  "owners": [ "Alexandre Mutel" ],
  "licenseUrl": "https://github.com/xoofx/AjaxMin/blob/master/license.txt",
  "projectUrl": "https://github.com/xoofx/AjaxMin",
  "requireLicenseAcceptance": false,
  "description": "A P/Invoke .NET wrapper around libsass to convert SCSS to CSS supporting NET2.0/NET3.5/NET4.x+ and CoreCLR platform",
  "copyright": "Alexandre Mutel",
  "tags": [ "libsass", "SCSS", "SASS", "CSS" ],
  "language": "en-US",
  "repository": {
    "type": "git",
    "url": "git://github.com/xoofx/AjaxMin"
  },
  "configurations": {
    "Debug": {
      "compilationOptions": {
        "define": [ "DEBUG", "TRACE" ],
        "allowUnsafe": true
      }
    },
    "Release": {
      "compilationOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true,
        "allowUnsafe": true,
        "xmlDoc": true,
        "keyFile": "key.snk",
        "strongName": true
      }
    }
  },
  "frameworks": {
    "net35": {
      "compilationOptions": {
        "define": [ "NET_35" ]
      },
      "frameworkAssemblies": {
        "mscorlib": "",
        "System": "",
        "System.Core": "",
        "System.Xml": ""
      }
    },
    "net40": {
      "frameworkAssemblies": {
        "mscorlib": "",
        "System": "",
        "System.Core": "",
        "System.Xml": ""
      }
    },
    "dotnet5.4": {
      "compilationOptions": {
        "define": [ "CORE" ]
      },
      "dependencies": {
        "Microsoft.NETCore": "5.0.1-beta-23516",
        "Microsoft.NETCore.Platforms": "1.0.1-beta-23516",
        "System.Runtime.Extensions": "4.0.11-beta-23516",
        "System.Reflection": "4.1.0-beta-23516",
        "System.Collections": "4.0.11-beta-23516",
        "System.Linq": "4.0.1-beta-23516",
        "System.Threading": "4.0.11-beta-23516",
        "System.Runtime": "4.0.21-beta-23516",
        "System.Xml.ReaderWriter": "4.0.11-beta-23516",
        "Microsoft.CSharp": "4.0.1-beta-23516",
        "System.Globalization.Extensions": "4.0.1-beta-23516",
        "System.Security.Cryptography.Encoding": "4.0.0-beta-23516",
        "System.Security.Cryptography.Algorithms":  "4.0.0-beta-23516"
      }
    }
  }
}